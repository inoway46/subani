<!-- Global site tag (gtag.js) - Google Analytics -->
<% ga_id = ENV["GA_ID"] %>
<script async src="https://www.googletagmanager.com/gtag/js?id=<%= ga_id %>"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', ga_id);
</script>