$('#<%= "#{@schedule.day}" + "#{@schedule.position}" %>').html('<%= link_to schedule_path(@schedule), method: :delete, data: { confirm: "削除しますか？" }, remote: true do %><i class="far fa-trash-alt float-right" style="padding:5px; color:gray;"></i><% end %>');
$('#<%= "#{@schedule.day}" + "#{@schedule.position}" %>').append('<%= link_to edit_schedule_path(@schedule.id), remote: true do %><i class="far fa-edit float-right" style="padding:5px; color:gray;"></i><% end %>');
$('#<%= "#{@schedule.day}" + "#{@schedule.position}" %>').append('<%= link_to @schedule.content.title, @schedule.content.url, target: :_blank, rel: "noopener noreferrer", type: "button", class: "btn btn-outline-primary", style: "font-size: 0.75rem;" %>');
$("#schedule-modal").modal("hide");
console.log('時間割を作成しました');